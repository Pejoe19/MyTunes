<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="249.0" prefWidth="473.0" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dk.easv.mytunes.GUI.PlaylistController">
   <children>
      <Label fx:id="lbNewEditPlaylist" layoutX="169.0" layoutY="14.0" prefHeight="27.0" prefWidth="145.0" text="New/Edit Playlist">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <TextField fx:id="txtNewPLName" layoutX="95.0" layoutY="95.0" prefHeight="47.0" prefWidth="322.0" style="-fx-border-color: black;" />
      <Label fx:id="lbName" layoutX="27.0" layoutY="107.0" prefHeight="23.0" prefWidth="58.0" text="Name:">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <Button fx:id="btnSave" layoutX="355.0" layoutY="178.0" mnemonicParsing="false" onAction="#onClickSave" text="Save">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Button>
      <Button fx:id="btnCancel" layoutX="256.0" layoutY="178.0" mnemonicParsing="false" onAction="#onClickCancel" text="Cancel">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
